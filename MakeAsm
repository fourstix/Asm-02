PROJECT = asm02
CC = CL
DEFS =
INCS =
INCDIR =
LIBS =
LIBDIR =
OBJS = \
	asm.obj ya_getopt.obj mmap.obj

$(PROJECT): $(OBJS)
	$(CC) $(DEFS) $(LIBDIR) $(OBJS) $(LIBS) -o $(PROJECT)

.c.obj:
	$(CC) $(DEFS) $(INCDIR) $(INCS) -c $<

clean:
	del *.obj *.exe

asm.obj:           header.h asm.c
ya_getopt.obj:		 ya_getopt.h ya_getopt.c
mmap.obj:					 mmap.h mmap.c
